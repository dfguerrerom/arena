  "scripts": {
    "build": "webpack -p --config webpack.config.js",
    "build-prod": "webpack --env production -p --config webpack.config.js",
    "build:watch": "webpack --config webpack.config.js --watch",
    "clean": "rm -rf dist/",
    "start": "node server/server.js",
    "postinstall": "webpack --config webpack.config.js",
    "watch": "cross-env-shell NODE_ENV=development \"yarn build:watch & yarn watch:server\"",
    "watch:server": "nodemon -e js --watch server server/server.js",
    "create-migration": "./server/db/migration/create-migration.sh",
    "test": "mocha-webpack --webpack-config webpack.config-test.js \"test/it/*.js\""
  },
